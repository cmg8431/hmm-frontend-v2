{
  "name": "hmm-frontend-v2",
  "private": true,
  "workspaces": {
    "packages": [
      "services/*"
    ]
  },
  "scripts": {
    "dev:web": "yarn workspace web run dev",
    "prepare": "husky install",
    "new": "scripts/init.sh"
  },
  "packageManager": "yarn@3.3.0",
  "devDependencies": {
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "hygen": "^6.2.11",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.3"
  },
  "lint-staged": {
    "**/*.+(ts|tsx|js|jsx)": [
      "eslint --fix --cache",
      "prettier --write"
    ]
  }
}
